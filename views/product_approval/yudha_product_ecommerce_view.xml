<odoo>
	<record id="product_ecommerce_view_tree" model="ir.ui.view">
		<field name="name">product.ecommerce.view.tree</field>
		<field name="model">product.ecommerce</field>
		<field name="arch" type="xml">
			<tree string="Master eCommerce List">
				<field name="name"/>
			</tree>
		</field>
	</record>

	<record id="product_ecommerce_view_form" model="ir.ui.view">
		<field name="name">product.ecommerce.view.form</field>
		<field name="model">product.ecommerce</field>
		<field name="arch" type="xml">
			<form string="Master eCommerce Form">
				<sheet>
					<group>
						<field name="name"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>
</odoo>
